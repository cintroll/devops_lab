apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: {{ certificate }}
    server: {{ endpoint }}
  name: {{ cluster_name }}
contexts:
- context:
    cluster: {{ cluster_name }}
    user: devops-service-account
  name: {{ cluster_name }}
current-context: {{ cluster_name }}
kind: Config
preferences: {}
users:
- name: devops-service-account
  user:
    token: {{ token }}
